{
  "semi": true,
  "singleQuote": false,
  "trailingComma": "es5",
  "tabWidth": 4,
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": false,
        "trailingComma": "none",
        "bracketSpacing": true,
        "arrowParens": "avoid",
        "proseWrap": "preserve",
        "quoteProps": "as-needed",
        "insertPragma": false,
        "jsxSingleQuote": false,
        "requirePragma": false,
        "useTabs": false
      }
    }
  ]
}
